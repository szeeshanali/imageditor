<div class="row mg-r-0">
    <div class="col-lg-3 pd-0">
        <div class="list-group widget-list-group bg-app-primary">
            <div class="pd-10">
              <!-- <div class="tx-14 tx-bold tx-uppercase tx-white tx-center pd-y-10">Manage ClipArts</div> -->
              <a href="#" id="btnBrowseClipart" class="btn btn-primary tx-bold btn-block tx-uppercase ">Browse Image</a>
              <input id="hiddenUploadClipArtFile" data-page-id="<%=locals.page.id%>" type="file" accept="image/*"  hidden="hidden">
              <input id="design-type" value="clipart" hidden="hidden">
            </div>
          </div>
          <div class="list-group-item ">
           <p >Upload JPEG, PNG or GIF Files</p>
          </div>
          <div class="card">
            <%- include('../../partials/admin/clipart-panel'); %>
          </div>
          
        <!-- <div class="card pd-10 tx-primary tx-bold bd-b">Upload Clipart</div>

        <div class="card pd-10" >
            <i class="tx-80  tx-gray-800 icon-imgupload ion-ios-cloud-upload-outline" id="admin-image-upload"></i>
    
            <p class="tx-12 pd-t-20 pd-x-30 tx-center">Upload (SVG,PNG) Cliparts.</p>
            <input id="admin-image-upload-hidden" data-page-id="<%=locals.page.id%>" type="file" accept="image/*" multiple="multiple" hidden="hidden">
            <input id="design-type" value="clipart" hidden="hidden">
        </div> -->
    </div>
    
    <div class="col-lg-6 pd-x-15">
      <div class="card pd-x-15 mg-b-15 pd-y-10 tx-primary tx-bold ">
        Manage ClipArts
       </div>  
        <div class="card " >
            <div class="workarea" >
                <div  id="canvas-holder">
                    <div class="content tx-center">
                        <canvas id="admin-main-canvas" width="500" height="400" style="margin-top:20px; background-color:transparent; border:solid 1px #ccc;  box-shadow: 3px 5px #ccc;" ></canvas>
                    </div>
                </div>
            </div>
        </div>
        
    </div>
  <div class="col-lg-3 pd-0">
    <div class="list-group widget-list-group bg-app-primary">
      <div class="list-group-item ">
        <div class="media d-block ">
       
          <div class="media-body mg-t-20 mg-sm-t-0">
            <span class=" tx-14 tx-uppercase">ClipArts </span>
            <span class="pull-right" style="background: white;
            color: #494699;
            border-radius: 50%;
            padding: 2px 5px;
            font-weight: bold;
            font-size: 12px;"><%=cliparts[0]?.count%></span>
          </div><!-- media-body -->
        </div><!-- media -->

      </div>
    </div>
    <div class="card" id="clipartAdminContainer" style="max-height: 500px; overflow-y: scroll;">
      <%cliparts?.forEach(clipart=>{ %>
       
        <div class="nav-item bd">
          <a href="#" class="nav-link with-sub text-primary tx-uppercase tx-12 tx-bold">
            <span><%=clipart.categoryName%> </span>
            <span class="pull-right badge badge-primary " ><%= clipart.items.length %></span>
          </a>
       
            <div class="nav-sub " style="display: none;">
              <div class="row mg-15" >

              <%clipart.items.forEach(item=>{ %>
                  <div id="<%=item.id %>" class=" nav-item col-lg-3 col-md-2 col-sm-6 col-xs-12 pd-0 clipart" data-active="<%=item.active %>" data-category="<%=clipart.categoryId %>" data-title="<%=item.title %>" data-url="<%=item.path %>" data-target="#clipartModal" data-toggle="modal">
                    <div>
                      <%if(!item.active){%>
                       <div class="tx-center tx-11 tx-uppercase tx-bold " style="color:red">Disabled</div>
                       <%}%> 
                       <img class="card-img-top img-fluid pd-b-10 " src="<%=item.path%>" alt="">

                      </div>
                  </div>
                <%})%>
              </div>
            </div>

       
              
            </div>
       
    
   <%})%>
    </div>
  </div>
    </div>
<script>
    $(".tab-content .tab-pane").each((i,e)=>
    { $(e).removeClass("active"); })    
   $("#clipartpanel").addClass("active");
$(document).ready(function(){
  canvasUndo.config.skipundo = true; 
})
  
</script>