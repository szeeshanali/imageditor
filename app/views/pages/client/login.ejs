<html lang="en">
    <head><%- include('../../partials/client/head'); %> </head>

  <body data-new-gr-c-s-check-loaded="14.1063.0" data-gr-ext-installed="">
    <form action="/app/login" method="POST">
    <div class="am-signin-wrapper">
        <%- include('../../partials/client/topbar',{user:null}); %>

      <div class="am-signin-box">
        <div class="row no-gutters">
          <div class="col-lg-5">
            <div>
              <img src="/images/icon.jpg">
              <h2>KakePrints.com</h2>
              <p>Please enter your <b>KopyKake</b> login information. <br>
                
                <br> Or click <a href="https://www.kopykake.com/register/">Sign up</a> to get your account registered at <b>KopyKake.com</b></p>
            </div>
          </div>
          <div class="col-lg-7">
            <%- include('../../partials/client/messages'); %>
            <br>
            <h5 class="tx-gray-800 mg-b-25">Sign In to Your Account</h5>
           
            <div class="form-group">
              <label class="form-control-label">Email:</label>
              <input type="email" name="email" value="<%=process.env.USR_EMAIL%>" class="form-control" placeholder="Enter your email address">
            </div><!-- form-group -->

            <div class="form-group">
              <label class="form-control-label">Password:</label>
              <input type="password" name="password" value="<%=process.env.USR_PASS%>" class="form-control" placeholder="Enter your password">
            </div><!-- form-group -->

            <label class="ckbox pd-b-10">
              <input type="checkbox" name="agreeterms" ><span><a href="#" style="text-decoration: underline" data-toggle="modal" data-target="#termspopup">I Agree with Terms and Conditions.</a></span>
            </label>

            <button type="submit" class="btn btn-block">Sign In</button>
            <label class="ckbox pd-b-10 pd-t-20">
              <a href="https://www.kopykake.com/password-reset/">Forget Password</a>
            </label>
           
            <!-- <div class="form-group mg-b-20"><a href="">Reset password</a></div> -->
          </div><!-- col-7 -->
        </div><!-- row -->
        <p class="tx-center  tx-12 mg-t-15">Copyright Â© <%=new Date().getFullYear();%>. KakePrints.com</p>
      </div><!-- signin-box -->
    </div><!-- am-signin-wrapper -->
</form>
<%- include('../../partials/client/popups/terms'); %>

<script src="/js/jquery.min.js"></script>
<script src="/js/jquery.js"></script>
<script src="/js/popper.js"></script>

<script src="/js/bootstrap.js"></script>
<script src="../js/amanda.js"></script>
  <script>
    
    $.get('/app/terms',(res)=>{
      $("#terms-contanier").html(res);
    })
</script>
  </script>

</body><grammarly-desktop-integration data-grammarly-shadow-root="true"></grammarly-desktop-integration></html>