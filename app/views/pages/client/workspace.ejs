                    <%for(var i=0; i<templates.length; i++){ %>
                      <%- include('../../partials/template',{template:templates[i]}); %>
                    <%}%>
                  </div>
              </div>
            </div>            
            <div id="uploadpanel" class="tab-pane"  >
