  
    <div class="card">
        <div class="pd-10 admin-design-ctrl">
            <button type="button" id="btnDeleteClipart" class="hidden btn btn-sm btn-primary tx-12 tx-bold tx-uppercase delete">Delete</button>

                <div class="pd-b-10">
                    <div class="tx-left">
                        <select id="admin-categories" class="form-control  select2 select2-hidden-accessible" data-placeholder="Choose one" tabindex="-1" aria-hidden="true">
                            <option label="Categories"></option>
                            <%for(var i=0; i<categories.length;i++){%>
                                <option value="<%=categories[i]._id%>"><%=categories[i].name%></option>
                           <%}%>
                          </select>
                        </div>
    
                  
                    <div class="mg-y-5">
                        <input class="form-control" placeholder="Title" id="admin-design-title" type="text">
                    </div>
                   
    


                    <div class="d-flex pd-y-10">
                        <label class="ckbox">
                            <input type="checkbox"   id="design-active"><span>Active</span>
                        </label>
                          <!-- <label class="ckbox mg-l-20">
                            <input type="checkbox" id="design-default" ><span>Default</span>
                          </label>
                       -->
                    </div>
    
            </div>
                <div id="save-image-ctrl" class="tx-center">
                    <div class="btn-group" role="group" >
                        <input type="hidden" id="selectedClipartId">
                        <button type="button" id="save-design" class="btn btn-primary  pd-x-25 active ">Save</button>
                        <button type="button" id="cancel-design" class="btn btn-secondary pd-x-25 ">Reset</button>

                      </div>
                </div>
            </div>
    </div>
  
