<div id="add-template-panel" class="" role="tabpanel" aria-labelledby="addtemplate">
  <div class="list-group widget-list-group bg-app-primary">    
    <div class="list-group widget-list-group bg-app-primary">
      <div class="list-group-item d-flex">
        <div class="media d-block d-sm-flex">
          <div class="d-block d-sm-flex mg-sm-r-20">
            <i class="ion-ios-keypad tx-24 tx-white"></i>
          </div><!-- d-flex -->
          <div class="media-body mg-t-10 mg-sm-t-0">
            <h6 class="mg-b-5 tx-14"><a href="" class="tx-white">Select Template</a></h6>
            <p class="mg-b-0 tx-12 tx-white-5">Next (Create Design)</p>
          </div><!-- media-body -->
        </div><!-- media -->
        <a href="#" id="btn-step-design" class="btn btn-primary pd-lg-x-20 mg-l-auto">Next</a>
      </div>
    </div>
  </div>
  <div class="row mg-0" id="customTemplateThumb">
    <%for(var i=0; i<templates.length; i++){ %>
    <div class="col-lg-4 pd-0  bd template" id="<%=templates[i].code %>">
      <div class="card  ht-200">
        <div class="card-body pd-10">
          <img class="card-img-top img-fluid pd-b-10 ht-140" src="<%=templates[i].base64 %>" alt="Image">
          <h5 class="tx-14 tx-bold tx-inverse  pd-x-10" style="height:28px;overflow:hidden"><%=templates[i].name %></h5>
          <p class="card-subtitle tx-12 tx-bold  pd-x-10"># <%=templates[i].ref_code %></p>
        </div>
      </div><!-- card -->
    </div>
    <%}%> 
  </div>


