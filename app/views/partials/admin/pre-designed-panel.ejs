<div id="create-design-panel" class="hidden">
  <div class="list-group widget-list-group bg-app-primary">
    <div class="list-group-item d-flex">
      <div class="media d-block d-sm-flex">
        <div class="d-block d-sm-flex mg-sm-r-20">
          <i class="ion-ios-keypad tx-24 tx-white"></i>
        </div><!-- d-flex -->
        <div class="media-body mg-t-10 mg-sm-t-0">
          <h6 class="mg-b-5 tx-14"><a href="" class="tx-white">Create Design</a></h6>
          <p class="mg-b-0 tx-12 tx-white-5">Add Photos, ClipArts, Text</p>
        </div><!-- media-body -->
      </div><!-- media -->
      <a href="#" id="btn-step-designx" class="btn btn-primary pd-lg-x-20 mg-l-auto">Preview</a>
    </div>
  </div>
  <ul class="nav nav-tabs tx-12 bold" role="tablist">
    <li class="nav-item" id="panel-photos">
      <a class="nav-link active " data-toggle="tab" href="#home"><i class="ion-images pd-x-5 tx-16"></i>PHOTOS</a>
    </li>
    <li class="nav-item" id="menu-clipart">
      <a class="nav-link" data-toggle="tab" href="#clipartmenu"><i class="ion-ios-paw pd-x-5 tx-16"></i> CLIP ART</a>
    </li>
    <li class="nav-item">
      <a class="nav-link" data-toggle="tab" href="#menu2"><i class="ion-social-tumblr pd-x-5 tx-16"></i> TEXT</a>
    </li>
  </ul>

  <!-- Tab panes -->
  <div class="tab-content">
    <div id="home" class="container tab-pane active">
      <div class="card pd-t-20 pd-0 tx-center " >
          <i class="tx-80  tx-gray-800 icon-imgupload ion-images" id="btn-upload-img"></i>
          <p class="tx-12 pd-t-20 pd-x-30 tx-center">Upload Photos </p>
          <input id="btn-upload-img-hidden" type="file" accept="image/*" multiple="multiple" hidden="hidden">
      </div>
    </div>
    <div id="clipartmenu" class="container tab-pane fade"><%- include('../../partials/client/clipart'); %>

    </div>
    <div id="menu2" class="container tab-pane fade"><br>
      <%- include('../../partials/client/typo'); %>           

    </div>
  </div>

      


 

