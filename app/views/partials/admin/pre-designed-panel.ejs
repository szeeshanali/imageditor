    </div>
   
    <div id="menu2" class="container tab-pane fade"><br>
      <%- include('../../partials/admin/typo'); %>           

    </div>
  </div>

      


 

</div>


<div id="save-design-panel" class="hidden">
  <div >
    <div class="list-group widget-list-group bg-app-primary">
      <div class="list-group-item d-flex">
        <div class="media d-block d-sm-flex">
          <div class="d-block d-sm-flex mg-sm-r-20">
            <i class="ion-android-done-all tx-24 tx-white"></i>
          </div><!-- d-flex -->
          <div class="media-body mg-t-10 mg-sm-t-0">
            <h6 class="mg-b-5 tx-14"><a href="" class="tx-white">Save Project</a></h6>
            <p class="mg-b-0 tx-12 tx-white-5">Active designs will be shared to all users.</p>
          </div><!-- media-body -->
        </div><!-- media -->
      </div>
    </div>

    <div class="card pd-20 ">
     
      <div class="form-layout">
        <div class="row ">
          <div class="col-lg-12">
            <div class="d-r-5">
              <label class="ckbox">
                  <input type="checkbox" id="designActive" ><span>Active</span>
                </label>
          </div>

            <div class="form-group">
              <label class="form-control-label">Project Name: <span class="tx-danger">*</span></label>
              <input class="form-control" required type="text" id="input-project-title" name="firstname"  >
            </div>
          </div>
          <div class="col-lg-12">
            <div class="form-group">
              <label class="form-control-label">Description:  </label>
              <textarea rows="2" class="form-control" id="input-project-desc" required  spellcheck="false"></textarea>
            </div>
          </div>

        </div><!-- row -->

        <div >
          <button class="btn  btn-primary mg-r-5" id="btnSave">Save Project</button>
          <button class="btn btn-secondary" id="btnCancelSaveDesign">Cancel</button>
        </div><!-- form-layout-footer -->
      </div><!-- form-layout -->
    </div>
    
    


  </div>
</div>
<div id="add-template-panel" class="" role="tabpanel" aria-labelledby="addtemplate">
  <div class="list-group widget-list-group bg-app-primary">
    <div class="list-group-item d-flex">
      <div class="media d-block d-sm-flex">
      
        <div class="media-body mg-t-10 mg-sm-t-0">
          <h6 class="mg-b-5 tx-18 tx-white">Choose Template</h6>
        </div>
         
        
        <!-- media-body -->
      </div><!-- media -->
       <div class="btn-group mg-b-0 mg-l-auto lh-2">
          <button type="button" class="btn btn-primary wd-120 ht-60 tx-18" id="btn-step-design">
            NEXT <br> <span class="tx-12 text-warning tx-bold underline">Create Design</span></button>
        </div>
    </div>
  </div>
  <div class="row mg-0" id="customTemplateThumb">
    <%for(var i=0; i<templates.length; i++){ %>
    <div class="col-lg-4 pd-0  bd template" id="<%=templates[i].code %>">
      <div class="card  ht-200">
        <div class="card-body pd-10">
          <img class="card-img-top img-fluid pd-b-10 ht-140" src="<%=templates[i].base64 %>" alt="Image">
          <h5 class="tx-14 tx-bold tx-inverse  pd-x-10" style="height:28px;overflow:hidden"><%=templates[i].name %></h5>
          <p class="card-subtitle tx-12 tx-bold  pd-x-10"># <%=templates[i].ref_code %></p>
        </div>
      </div><!-- card -->
    </div>
    <%}%> 
  </div>

</div>
   
<script>
  $("#menu-save-design").removeClass("hidden");
</script>