   



<div class="row" >

  <div class="col-lg-12  " >

    <div class="nav am-sideleft-menu pd-0">
    
      <%cliparts?.forEach(clipart=>{ %>
       
        <div class="nav-item">
          <a href="#" class="nav-link with-sub text-primary tx-12 tx-bold tx-uppercase">
            <span><%=clipart.categoryName%> </span><span class="tx-bold mg-l-10">(<%= clipart.items.length %>)</span>
          </a>
       
            <div class="nav-sub" style="display: none;">
              <div class="row" id="clipartContainer">

              <%clipart.items.forEach(item=>{ %>
              
                <div class=" nav-item col-lg-3 col-md-3 col-sm-6 col-xs-12 pd-2   clipart" data-title="<%=item.title %>" data-url="<%=item.path%>" data-target="#clipartModal" data-toggle="modal">
                  <div  >
                      <img class="card-img-top img-fluid pd-b-10 " src="<%=item.path%>" alt="">
                 
                  </div>
              </div>
                
                <%})%>
              </div>
            </div>

       
              
            </div>
       
    
   <%})%>
      </div>
  </div>


      
    </div>

